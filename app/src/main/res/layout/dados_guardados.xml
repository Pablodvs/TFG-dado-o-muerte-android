    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/dadoG"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:textColor="@color/black" />

    <Button
